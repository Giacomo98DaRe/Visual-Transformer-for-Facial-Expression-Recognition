# Configuration file describing how to run a proper docker container.

# NVIDIA-Docker run options
docker_args: -v /home/dare/datasets/private/face/AffectNet8/data:/data

# Run script options
# To run the container multiple times
num_iters: 1

# The name of containers (remember you have to prefix it with '{user}_')
container_name: {user}_MyAffectNet_{args.gpu}_{date}

# The docker image to use
image_name: dare/dr:facenet_image
